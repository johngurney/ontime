Import users
<table align="left" style="background-color: lightblue; border-style: solid; border-width: 1px;" width="70%" height="50">

  <col width="25%">
  <col width="10%">
  <col width="10%">
  <col width="15%">
  <col width="10%">
  <col width="30%">

<%= form_tag(import_users_path) do %>
  <% if @tempusers %>
    <% @tempusers.each do |user| %>
      <tr>
      <td align="left">
      <%=check_box_tag "check" + user.id.to_s, "1" , 1 %>
        <%= link_to user.first_name + " " + user.last_name, edit_temp_user_path(user) %>
      </td>
      <td align="left">
        <%= user.team %>
      </td>
      <td align="left">
        <%= user.office%>
      </td>
      <td align="left">
        <%= user.position %>
      </td>
      <td align="left">
        <%= user.experience %>
      </td>
      <td align="left">
        <%= user.email %>
      </td>
    </tr>
    <% end %>
    <%= submit_tag "Import" %>
  <% end %>
<% end %>
</table>
